<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>

  <servlet>
    <servlet-name>ServletConfigDemo1</servlet-name>
    <servlet-class>com.web.servlet.config.ServletConfigDemo1</servlet-class>
    <!--配置ServletConfigDemo1的初始化参数 -->
    <init-param>
      <param-name>name</param-name>
      <param-value>an</param-value>
    </init-param>
    <init-param>
      <param-name>password</param-name>
      <param-value>123</param-value>
    </init-param>
    <init-param>
      <param-name>charset</param-name>
      <param-value>UTF-8</param-value>
    </init-param>
  </servlet>


  <servlet>
    <servlet-name>ServletDemo1</servlet-name>
    <servlet-class>com.web.servlet.ServletDemo1</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>DrawImage</servlet-name>
    <servlet-class>com.web.servlet.drawImage.DrawImage</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>CheckServlet</servlet-name>
    <servlet-class>com.web.servlet.drawImage.CheckServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>CookieDemo</servlet-name>
    <servlet-class>com.web.servlet.session.CookieDemo</servlet-class>
  </servlet>



  <servlet>
    <servlet-name>ServletDemo3</servlet-name>
    <servlet-class>com.web.servlet.ServletDemo3</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>HttpResponseRedirect</servlet-name>
    <servlet-class>com.web.servlet.http.HttpResponseRedirect</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>HttpResponseZip</servlet-name>
    <servlet-class>com.web.servlet.http.HttpResponseZip</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>HttpResponseDatatype</servlet-name>
    <servlet-class>com.web.servlet.http.HttpResponseDatatype</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>FormServletNew</servlet-name>
    <servlet-class>com.web.servlet.session.FormServletNew</servlet-class>
  </servlet>


  <servlet>
    <servlet-name>DoFormServlet</servlet-name>
    <servlet-class>com.web.servlet.session.DoFormServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>ServletDemo1</servlet-name>
    <url-pattern>/ServletDemo1</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>ServletDemo3</servlet-name>
    <url-pattern>/ServletDemo3</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>HttpResponseRedirect</servlet-name>
    <url-pattern>/HttpResponseRedirect</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>HttpResponseZip</servlet-name>
    <url-pattern>/HttpResponseZip</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>HttpResponseDatatype</servlet-name>
    <url-pattern>/HttpResponseDatatype</url-pattern>
  </servlet-mapping>


  <servlet-mapping>
    <servlet-name>ServletConfigDemo1</servlet-name>
    <url-pattern>/ServletConfigDemo1</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>DrawImage</servlet-name>
    <url-pattern>/servlet/DrawImage</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>CheckServlet</servlet-name>
    <url-pattern>/servlet/CheckServlet</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>CookieDemo</servlet-name>
    <url-pattern>/servlet/CookieDemo</url-pattern>
  </servlet-mapping>


  <servlet-mapping>
    <servlet-name>FormServletNew</servlet-name>
    <url-pattern>/servlet/FormServletNew</url-pattern>
  </servlet-mapping>



  <servlet-mapping>
    <servlet-name>DoFormServlet</servlet-name>
    <url-pattern>/servlet/DoFormServlet</url-pattern>
  </servlet-mapping>

  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
